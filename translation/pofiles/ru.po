#~ "\n"
#~ "\n"
#~ "    (rule \"clearance-to-1mm-cutout\"\n"
#~ "        (constraint clearance (min 0.8mm))\n"
#~ "        (condition \"A.Layer == 'Edge.Cuts' && A.Thickness == 1.0mm\"))\n"
#~ "\n"
#~ "\n"
#~ "    (rule \"Max Drill Hole Size Mechanical\"\n"
