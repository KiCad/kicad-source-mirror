#~ "<br>\n"
#~ "\n"
#~ "### Zone Connections\n"
#~ "\n"
#~ " * solid\n"
#~ " * thermal\\_reliefs\n"
#~ " * none\n"
#~ "\n"
#~ "<br>\n"
#~ "\n"
#~ "### Severity Names\n"
#~ "\n"
#~ " * warning\n"
#~ " * error\n"
#~ " * exclusion\n"
#~ " * ignore\n"
#~ "\n"
#~ "<br>\n"
#~ "\n"
#~ "### Examples\n"
#~ "\n"
#~ "    (version 1)\n"
#~ "\n"
#~ "    (rule HV\n"
#~ "       (constraint clearance (min 1.5mm))\n"
#~ "       (condition \"A.NetClass == 'HV'\"))\n"
#~ "\n"
#~ "\n"
#~ "    (rule HV\n"
#~ "       (layer outer)\n"
#~ "       (constraint clearance (min 1.5mm))\n"
