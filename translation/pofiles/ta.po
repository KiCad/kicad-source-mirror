msgstr "பட உயரம்"

#: kicad/cli/command_pcb_render.cpp:237
#, c-format
msgid "Render from side. Options: %s"
msgstr "பக்கத்திலிருந்து வழங்கவும். விருப்பங்கள்: %s"

#: kicad/cli/command_pcb_render.cpp:242
#, c-format
msgid ""
