#~ "       (constraint clearance (min 0.05mm) (opt 0.08mm))\n"
#~ "       (condition \"A.intersectsCourtyard('U3')\"))\n"
#~ "\n"
#~ "\n"
#~ "    # prevent silk over tented vias\n"
#~ "    (rule silk_over_via\n"
