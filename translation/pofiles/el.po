#~ "    (severity <severity_name>)\n"
#~ "\n"
#~ "\n"
#~ "<br>\n"
#~ "\n"
#~ "### Constraint Types\n"
#~ "\n"
#~ " * annular\\_width\n"
#~ " * assertion\n"
#~ " * clearance\n"
#~ " * connection\\_width\n"
#~ " * courtyard_clearance\n"
