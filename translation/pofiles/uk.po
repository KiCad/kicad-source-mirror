"        (обмеження clearance (min 0.2mm))\n"
"        (умова \"A.Type == 'Pad' && B.Type == 'Track'\"))\n"
"\n"
"\n"
"    (правило «clearance-to-1mm-cutout»\n"
"        (обмеження edge_clearance (min 0,8 мм))\n"
"        (умова «A.Layer == 'Edge.Cuts' && A.Line_Width == 1,0 мм»))\n"
"\n"
"\n"
"    (правило «Максимальний розмір отвору для свердління механічний»\n"
"        (обмеження розмір_отвору (макс. 6,3 мм))\n"
