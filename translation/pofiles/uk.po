#~ "    (константи <constraint_type> ...)\n"
#~ "\n"
#~ "    (застереження\"<expression>\")\n"
#~ "\n"
#~ "    (шар \"<layer_name>\")\n"
#~ "\n"
#~ "    (ретельність <severity_name>)\n"
#~ "\n"
#~ "\n"
#~ "<br>\n"
#~ "\n"
#~ "### Типи констант\n"
