#~ "    (rule front_mechanical_board_edge_clearance\n"
#~ "        (layer \"F.Courtyard\")\n"
#~ "        (constraint physical_clearance (min 3mm))\n"
#~ "        (condition \"B.Layer == 'Edge.Cuts'\"))\n"
#~ "\n"
#~ "\n"
#~ "    # Verificar la capacidad de manejo de corriente\n"
