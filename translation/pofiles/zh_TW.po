#~ "\n"
#~ "\n"
#~ "    # Prevent solder wicking from SMD pads\n"
#~ "    (rule holes_in_pads\n"
#~ "        (constraint physical_hole_clearance (min 0.2mm))\n"
#~ "        (condition \"B.Pad_Type == 'SMD'\"))\n"
